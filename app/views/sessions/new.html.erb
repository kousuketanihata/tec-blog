<% provide(:title, "Sign in") %>
<h1>Sign in</h1>
<%= form_for(:session, url: login_path) do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit(['送信']) %>
<% end %>

<% @current_user %>